<h1>Artists</h1>
<ul>
  <% @artists.each do |artist| %>
    <li><%= link_to artist.name, artist_path(artist) %></li>
      <% artist.songs.each do |song| %>
        <div><%= link_to song.artist_name, artist_songs_path(artist) %></div>
        <div><%= link_to song.title, song_path(song) %></div>
        <% end %>
  <% end %>
</ul>
